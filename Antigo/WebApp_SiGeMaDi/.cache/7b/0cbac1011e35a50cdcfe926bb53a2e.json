ype_Managment.js","original":{"line":61,"column":15}}],"sources":{"components/LabManager/Type_Managment.js":"import React, { Component } from 'react'\r\nimport Link from 'react-router-dom'\r\n\r\nclass Type_Managment extends Component {\r\n\r\n\r\n    state = {\r\n        types:[{id:'1',name:'ATB'},{id:'2',name:'PDS16'},{id:'3',name:'BreadBoard'},{id:'4',name:'Cabos'},{id:'5',name:'Chips'}]\r\n    }\r\n\r\n    onSubmit() {\r\n        //adicionar novo material\r\n\r\n        this.props.history.push('/auth/labmanager/add')\r\n    }\r\n\r\n    deleteTypes(id) {\r\n\r\n        let types = this.state.types.reduce((types, type) => {\r\n            if (type.id != id) {\r\n                types.push(type)\r\n            }\r\n            return types\r\n        }, [])\r\n\r\n        //SE FIZER A FILTRAGEM E APAGAR UM ELE NAO SE REFLETE AI SO SE FIZER RESET NO FILTRO PQ EU ESTOU A AFETAR MATERIAL E NAO FILTERED\r\n        //MAS COMO ISTO VAI TER PEDIDOS NAO HA PROBLEMA PQ VOU BUSCAR OUTRA VEZ COM ESSE FILTRO E DEPOIS JA NAO APARECE O MATERIAL\r\n\r\n        //fazer pedido de delete\r\n        this.setState({ types: [...types] })\r\n    }\r\n\r\n    createTypesList() {\r\n\r\n        return this.state.types.map(type => {\r\n            return <div className=\"item\" key={type.id}>\r\n                <div className=\"right floated content\">\r\n                    <button className=\"big ui compact icon button\" onClick={() => this.deleteType(type.id)}><i className=\"red close icon\"></i></button>\r\n                </div>\r\n                <div className=\"content\">\r\n                    <Link to={this.props.location.pathname + '/' + type.id}>\r\n                        {type.name}\r\n                    </Link>\r\n                </div>\r\n            </div>\r\n\r\n        })\r\n    }\r\n\r\n    render() {\r\n        return (\r\n            <div>\r\n                <h3 className=\"ui medium header\">Material:</h3>\r\n                <button className=\"ui medium labeled icon button\" onClick={this.onSubmit}><i className=\"check circle icon\"></i>Adicionar Material</button >\r\n                <hr />\r\n                {this.createTypesList()}\r\n            </div>\r\n        )\r\n    }\r\n}\r\nexport default Type_Managment"},"lineCount":null}},"hash":"c4ef4b5ccbf1150cc64c91f57d3f1cb7","cacheData":{"env":{}}}