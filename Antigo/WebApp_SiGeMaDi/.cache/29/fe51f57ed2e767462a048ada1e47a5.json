n":7},"source":"components/Teacher/Reservation.js","original":{"line":35,"column":8}},{"generated":{"line":80,"column":8},"source":"components/Teacher/Reservation.js","original":{"line":34,"column":4}},{"generated":{"line":81,"column":0},"source":"components/Teacher/Reservation.js","original":{"line":64,"column":1}},{"name":"Reservation","generated":{"line":83,"column":15},"source":"components/Teacher/Reservation.js","original":{"line":66,"column":15}}],"sources":{"components/Teacher/Reservation.js":"import React, { useState, useEffect } from 'react'\r\nimport { Button, Header, List, Divider } from 'semantic-ui-react'\r\nimport { reservationsUserUrl } from '../Links'\r\nimport axios from 'axios'\r\nimport Response_Handler from '../ResponseHandler'\r\nimport { SemanticToastContainer } from 'react-semantic-toasts'\r\nimport Cookies from 'universal-cookie'\r\n\r\nfunction Reservation(props) {\r\n\r\n    const [reservations, setReservations] = useState([])\r\n    const [error, setError] = useState(null)\r\n\r\n    useEffect(() => {\r\n        const id = JSON.parse(sessionStorage.getItem('userinfo')).id\r\n        axios.get(reservationsUserUrl.replace(':id', id))\r\n            .then(resp => {\r\n                setReservations(resp.data['reservations'])\r\n            })\r\n            .catch(err => setError(Response_Handler(err.response)))\r\n    }, [])\r\n\r\n    function newRequest() {\r\n        props.history.push('/auth/teacher/newreservation')\r\n    }\r\n\r\n\r\n    function onClickRequest(id) {\r\n        props.history.push(props.location.pathname + `/${id}`)\r\n    }\r\n\r\n\r\n\r\n    return (\r\n        <div>\r\n            <SemanticToastContainer />\r\n            {\r\n                error ? error :\r\n                    <div>\r\n                        <Header size='medium'>Reservations</Header>\r\n                        <Button basic size='large' onClick={newRequest} icon='file alternate' content='New Reservation' />\r\n                        <div style={{ display: 'block' }}>\r\n                            <Divider />\r\n                            <List selection>\r\n                                {\r\n                                    reservations.length === 0 ? <Header>There is no Active Reservations</Header> :\r\n                                        reservations.map(request => {\r\n                                            return <List.Item key={request.id} onClick={() => onClickRequest(request.id)}>\r\n                                                <List.Content>\r\n                                                    <List.Header>{request.id}</List.Header>\r\n                                                    <List.Description>{request.subject} | {request.date} | {request.hour} </List.Description>\r\n                                                </List.Content>\r\n                                            </List.Item>\r\n\r\n                                        })\r\n                                }\r\n                            </List>\r\n                        </div>\r\n                    </div>\r\n            }\r\n\r\n        </div>\r\n    )\r\n}\r\n\r\nexport default Reservation"},"lineCount":null}},"hash":"16a39757b174a516c46008a60ec309e2","cacheData":{"env":{}}}